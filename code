		system("cls");
		cout<<"再放 3.蛋  2.神位上的米饭 1.米饭";
		cin>>dandan; 
		if(dandan == 1) {
		system("cls");
		while(99999999){
		cout<<"恭喜通关"; 
	} 
	}else{
		MessageBox(NULL, "不能用隔夜米饭", "you Ⅳ 了", MB_OK);
	}
	}else{
		MessageBox(NULL, "不会做蛋炒饭", "you Ⅳ 了", MB_OK);
